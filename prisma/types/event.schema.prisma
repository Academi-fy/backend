model Event {
  id        String   @id @default(uuid()) @db.Uuid
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  clubs       Club[]
  description String?
  endDate     DateTime
  host        String
  information Json[]
  location    String?
  school      School        @relation(fields: [schoolId], references: [id])
  schoolId    String @db.Uuid
  startDate   DateTime
  state       String
  subscribers User[]
  tickets     EventTicket[]
  title       String

  @@map("events")
}